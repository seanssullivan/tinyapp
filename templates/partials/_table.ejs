<h3>My URLs</h3>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Short URL</th>
        <th scope="col">Long URL</th>
        <th scope="col">Edit</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>
      <% for(let url of urls) { %>
        <tr>
          <td><%= url.shortURL %></td>
          <td><%= url.longURL %></td>
          <td><a class="btn btn-outline-primary btn-sm" href="/urls/<%= url %>">Edit</a></td>
          <td>
            <form action="/urls/<%= url %>/delete" method="POST">
              <button class="btn btn-outline-danger btn-sm">Delete</input>
            </form>
          </td>
        </tr>
      <% }%>
    </tbody>
  </table>